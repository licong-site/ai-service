{
  "name": "ai-service",
  "version": "1.0.0",
  "description": "AI服务后端 - Cloudflare Worker连接DeepSeek API",
  "main": "src/index.ts",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:prod": "wrangler deploy --env production",
    "tail": "wrangler tail",
    "tail:prod": "wrangler tail --env production",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "pnpm run clean && pnpm run type-check",
    "build": "wrangler deploy --dry-run",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "deepseek",
    "ai",
    "chat",
    "api",
    "typescript",
    "serverless",
    "pnpm"
  ],
  "author": "向暖",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240112.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "pnpm": {
    "overrides": {
      "@types/node": "^20.0.0"
    }
  }
}